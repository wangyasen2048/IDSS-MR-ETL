package com.cucrz.idss.hadoop.etl.exception;

/**
 * @author 方云
 * 
 */
public class IdsshvException extends Exception {
	private static final long serialVersionUID = 3912018960722264231L;

	public IdsshvException() {
		super();
	}

	public IdsshvException(String message, Throwable cause) {
		super(message, cause);
	}

	public IdsshvException(String message) {
		super(message);
	}

	public IdsshvException(Throwable cause) {
		super(cause);
	}

}
